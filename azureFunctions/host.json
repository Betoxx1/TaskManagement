{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "api"
        }
    },
    "cors": {
        "allowedOrigins": [
            "http://localhost:3000",
            "http://127.0.0.1:3000"
        ],
        "supportCredentials": true
    },
    "functionTimeout": "00:05:00"
}